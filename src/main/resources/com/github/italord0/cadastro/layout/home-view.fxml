<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="537.0" prefWidth="1007.0" spacing="20.0" xmlns="http://javafx.com/javafx/21"
      xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.github.italord0.cadastro.controller.HomeController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>
    <TableView fx:id="usersTableView" prefHeight="584.0" prefWidth="685.0" tableMenuButtonVisible="true">
        <columns>
            <TableColumn prefWidth="50.0" text="Id" fx:id="idColumn"/>
            <TableColumn prefWidth="200.0" text="Name" fx:id="nameColumn"/>
            <TableColumn minWidth="100.0" prefWidth="150.0" text="E-Mail" fx:id="emailColumn"/>
            <TableColumn prefWidth="100.0" text="Date of Birth" fx:id="dateOfBirthColumn"/>
            <TableColumn prefWidth="100.0" text="Balance" fx:id="balanceColumn"/>
        </columns>
    </TableView>
    <HBox alignment="CENTER" minWidth="-Infinity" prefHeight="100.0" prefWidth="1280.0">
        <HBox minWidth="-Infinity" nodeOrientation="LEFT_TO_RIGHT" prefHeight="100.0" prefWidth="600.0">
            <TextField/>
            <TextField/>
            <TextField/>
        </HBox>
        <HBox alignment="TOP_RIGHT" nodeOrientation="LEFT_TO_RIGHT" prefHeight="100.0" prefWidth="200.0">
            <Button mnemonicParsing="false" text="Button"/>
            <Button mnemonicParsing="false" text="Button"/>
            <Button mnemonicParsing="false" text="Button"/>
        </HBox>
    </HBox>
</VBox>
